{
    "name": "l3aro/cloudflare-turnstile",
    "description": "Filament Turnstile is an essential plugin designed to seamlessly integrate Cloudflareyour Filament application.",
    "keywords": [
        "l3aro",
        "laravel",
        "cloudflare-turnstile"
    ],
    "homepage": "https://github.com/l3aro/cloudflare-turnstile",
    "support": {
        "issues": "https://github.com/l3aro/cloudflare-turnstile/issues",
        "source": "https://github.com/l3aro/cloudflare-turnstile"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "l3aro",
            "email": "dgbao1340@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.92",
        "illuminate/contracts": "^11.0|^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "l3aro\\CloudflareTurnstile\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "l3aro\\CloudflareTurnstile\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "l3aro\\CloudflareTurnstile\\CloudflareTurnstileServiceProvider"
            ],
            "aliases": {
                "CloudflareTurnstile": "l3aro\\CloudflareTurnstile\\Facades\\CloudflareTurnstile"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
