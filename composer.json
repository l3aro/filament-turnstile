{
    "name": "l3aro/filament-turnstile",
    "description": "Filament Turnstile is an essential plugin designed to seamlessly integrate Cloudflare Turnstile into your Filament application.",
    "keywords": [
        "l3aro",
        "laravel",
        "filament",
        "cloudflare-turnstile",
        "filament-turnstile"
    ],
    "homepage": "https://github.com/l3aro/filament-turnstile",
    "support": {
        "issues": "https://github.com/l3aro/filament-turnstile/issues",
        "source": "https://github.com/l3aro/filament-turnstile"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "l3aro",
            "email": "dgbao1340@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0|^5.0",
        "spatie/laravel-package-tools": "^1.92",
        "illuminate/contracts": "^11.0|^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0|^8.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^3.0|^4.0",
        "pestphp/pest-plugin-arch": "^3.0|^4.0",
        "pestphp/pest-plugin-laravel": "^3.0|^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "l3aro\\FilamentTurnstile\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "l3aro\\FilamentTurnstile\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "l3aro\\FilamentTurnstile\\FilamentTurnstileServiceProvider"
            ],
            "aliases": {
                "FilamentTurnstile": "l3aro\\FilamentTurnstile\\Facades\\FilamentTurnstile"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
